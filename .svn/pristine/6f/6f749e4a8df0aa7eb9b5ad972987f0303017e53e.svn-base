package com.crm.domain;

public class Relationship_Record {
	private int rCid;
	private String rCtext;
	private String rCdate;
	private int clientId;
	private int cLMid;
	private String rCtype;
	private int myLMid;
	public Relationship_Record() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Relationship_Record(int rCid, String rCtext, String rCdate,
			int clientId, int cLMid, String rCtype, int myLMid) {
		super();
		this.rCid = rCid;
		this.rCtext = rCtext;
		this.rCdate = rCdate;
		this.clientId = clientId;
		this.cLMid = cLMid;
		this.rCtype = rCtype;
		this.myLMid = myLMid;
	}
	public int getrCid() {
		return rCid;
	}
	public void setrCid(int rCid) {
		this.rCid = rCid;
	}
	public String getrCtext() {
		return rCtext;
	}
	public void setrCtext(String rCtext) {
		this.rCtext = rCtext;
	}
	public String getrCdate() {
		return rCdate;
	}
	public void setrCdate(String rCdate) {
		this.rCdate = rCdate;
	}
	public int getClientId() {
		return clientId;
	}
	public void setClientId(int clientId) {
		this.clientId = clientId;
	}
	public int getcLMid() {
		return cLMid;
	}
	public void setcLMid(int cLMid) {
		this.cLMid = cLMid;
	}
	public String getrCtype() {
		return rCtype;
	}
	public void setrCtype(String rCtype) {
		this.rCtype = rCtype;
	}
	public int getMyLMid() {
		return myLMid;
	}
	public void setMyLMid(int myLMid) {
		this.myLMid = myLMid;
	}

	
	
}
